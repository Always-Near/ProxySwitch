{
  "name": "ProxySwitch",
  "description": "Proxy Switch",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "permissions": [
    "tabs",
    "proxy"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/styles/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "action": {
    "default_title": "Proxy Switch",
    "default_popup": "popup.html"
  }
}